   ],
   "source": [
    "import pandas as pd\n",
    "import matplotlib.pyplot as plt\n",
    "\n",
    "data = {\n",
    "    'Ticker': ['NVDA', 'TSLA', 'AAPL', 'AMZN', 'GOOGL'],\n",
    "    'Price_Yesterday': [130.5, 240.0, 190.0, 145.0, 138.0],\n",
    "    'Price_Today':     [135.2, 235.0, 192.5, 148.0, 137.5],\n",
    "    'Sector': ['AI', 'Auto', 'Tech', 'E-comm', 'Search']\n",
    "}\n",
    "\n",
    "df = pd.read_csv('stock_data.csv') if False else pd.DataFrame(data) \n",
    "\n",
    "print(\"--- 1. 原始市场数据 ---\")\n",
    "display(df)\n",
    "\n",
    "df['Return_Pct'] = (df['Price_Today'] - df['Price_Yesterday']) / df['Price_Yesterday'] * 100\n",
    "\n",
    "\n",
    "df['Profit_Per_Share'] = df['Price_Today'] - df['Price_Yesterday']\n",
    "\n",
    "print(\"\\n--- 2. 计算完指标后的数据 ---\")\n",
    "display(df)\n",
    "\n",
    "\n",
    "buy_signal = df[df['Return_Pct'] > 1.0]\n",
    "\n",
    "print(\"\\n--- 3. 交易信号：AI 建议买入这些 ---\")\n",
    "display(buy_signal)\n",
    "\n",
    "plt.figure(figsize=(8, 5))\n",
    "\n",
    "plt.bar(df['Ticker'], df['Return_Pct'], color='skyblue')\n",
    "plt.axhline(0, color='grey', linewidth=0.8) # 画一条 0 轴线\n",
    "plt.title('Daily Stock Returns')\n",
    "plt.ylabel('Return (%)')\n",
    "plt.show()\n",
    "\n",
    "buy_signal.to_csv('buy_orders.csv', index=False)\n",
    "\n",
    "print(\"策略执行完毕！结果已保存为 'buy_orders.csv'\")"
   ]
  },
